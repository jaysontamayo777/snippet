{
  "name": "ecommerce-frontend-travel-ph",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "clear-build": "rimraf build-sit && rimraf build-uat && rimraf build-production",
    "build": "cross-env PUBLIC_URL=http://no-domain.com npm run build-task",
    "build-task": "npm run clear-build && npm run build-css && npm run build-sit && npm run build-uat && npm run build-prod",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-js": "react-scripts build",
    "build-prod": "cross-env REACT_APP_LOCATION=production npm run buildScript",
    "build-uat": "cross-env REACT_APP_LOCATION=uat npm run buildScript",
    "build-sit": "cross-env REACT_APP_LOCATION=sit npm run buildScript",
    "buildScript": "react-scripts build && npm run replace-css && npm run replace-fonts && npm run replace-work-env && replace ${PUBLIC_URL}/ '' build/* && mv build build-${REACT_APP_LOCATION}",
    "cover": "npm test -- --coverage",
    "import:localization": "node ./tools/importConfluenceLocalization.js",
    "postcover": "cp coverage/coverage-final.json coverage/coverage.json",
    "eject": "react-scripts eject",
    "replace-css": "replace ${PUBLIC_URL}/static '..' build/static/css/* && replace /static/images ../images build/static/css/* && replace ${PUBLIC_URL}/static '..' build/static/js/* && replace /static/images ../images build/static/js/*",
    "replace-fonts": "replace ${PUBLIC_URL}/static '..' build/static/css/* && replace /static/fonts ../fonts build/static/css/* && replace ${PUBLIC_URL}/static '..' build/static/js/* && replace /static/fonts ../fonts build/static/js/*",
    "replace-work-env": "replace WORK_ENV ${REACT_APP_LOCATION} build/index.html",
    "start": "node server.js",
    "lint": "eslint src tools --color",
    "lint:watch": "esw --ignore-path .eslintignore . --watch",
    "start-dev": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "npm run test:spec -- --coverage",
    "test:spec": "cross-env NODE_ENV=test CI=true && react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "storybook": "start-storybook -p 9001 -c storybook"
  },
  "dependencies": {
    "@axa-asia/frontend": "^4.53.0",
    "@axa-asia/react-web-toolkit": "^2.6.8",
    "@axa-asia/ecommerce-field-validator-ph": "^2.2.3",
    "@axa-asia/stp-field-validator-travel-agi": "^0.2.2",
    "@axa/web-toolkit": "^2.0.0-alpha.3",
    "axios": "^0.18.0",
    "axios-retry": "^3.1.1",
    "bootstrap": "^4.2.1",
    "browser-detect": "^0.2.28",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "confluence-api": "^1.3.0",
    "cookie-parser": "^1.4.3",
    "currency-formatter": "^1.4.3",
    "decode-uri-component": "^0.2.0",
    "dotenv": "^6.2.0",
    "express-basic-auth": "^1.1.5",
    "express-package-version": "^0.0.2",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "getenv": "^0.7.0",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "html-react-parser": "^0.4.6",
    "jsonfile": "^4.0.0",
    "mdn-polyfills": "^5.10.0",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.4.0",
    "querystring": "^0.2.0",
    "react": "^16.4.1",
    "react-bootstrap": "0.32.0",
    "react-datetime": "^2.9.0",
    "react-dom": "^16.4.1",
    "react-grid-system": "^4.1.5",
    "react-helmet": "^5.2.0",
    "react-html-parser": "^2.0.2",
    "react-iframe": "^1.3.3",
    "react-iframe-comm": "^1.2.2",
    "react-redux": "^5.0.7",
    "react-redux-i18n": "^1.9.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "2.1.3",
    "react-select": "^1.3.0",
    "react-sticky-el": "^1.0.20",
    "react-widgets": "^4.4.10",
    "react-widgets-moment": "^4.0.26",
    "react-window-resize-listener": "^1.1.0",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "4.6.0",
    "redux-simple-storage-middleware": "^2.2.1",
    "redux-thunk": "^2.3.0",
    "smoothscroll": "^0.4.0",
    "strict-uri-encode": "^2.0.0",
    "styled-components": "^4.1.3",
    "typescript": "^3.3.3",
    "uuid": "^3.3.2",
    "validator": "^10.5.0",
    "webpack": "4.19.1",
    "window-resize-listener-react": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/react": "^4.1.11",
    "babel-loader": "8.0.4",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "mocha": "^5.2.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.2.0",
    "redux-mock-store": "^1.5.3",
    "replace": "^1.0.0",
    "rimraf": "^2.6.2",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
